{
    "name": "goodid/goodid-php-sdk",
    "description": "GoodID SDK for PHP",
    "keywords": ["goodid", "sdk"],
    "type": "library",
    "homepage": "https://github.com/idandtrust/goodid-php-sdk",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-openssl": "*",
        "ext-json": "*",
        "paragonie/random_compat": "^1|^2",
        "jwadhams/json-logic-php": "^1.3",
        "spomky-labs/base64url": "^1.0",
        "web-token/jwt-framework": "^3.3 || ^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0 || ^9.0",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "GoodID\\": "src/GoodID/",
            "GoodIDPass\\": "src/GoodIDPass/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoodID\\": "tests/unit/GoodID/"
        }
    }
}
